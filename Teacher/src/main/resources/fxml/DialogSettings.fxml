<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="117.0" prefWidth="331.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.teacher.gui.ControllerDialogSettings">
   <bottom>
      <FlowPane alignment="CENTER_RIGHT" columnHalignment="CENTER" BorderPane.alignment="CENTER_RIGHT">
         <children>
            <Button fx:id="buttonOk" mnemonicParsing="false" onAction="#actionButtonOk" prefHeight="25.0" prefWidth="108.0" text="%key.buttonOk">
               <FlowPane.margin>
                  <Insets right="3.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="buttonCancel" mnemonicParsing="false" onAction="#actionButtonCancel" prefHeight="25.0" prefWidth="105.0" text="%key.buttonCancel">
               <FlowPane.margin>
                  <Insets left="3.0" />
               </FlowPane.margin>
            </Button>
         </children>
         <BorderPane.margin>
            <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
         </BorderPane.margin>
      </FlowPane>
   </bottom>
   <center>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER_RIGHT">
               <children>
                  <Label fx:id="labelLanguge" text="%key.labelLanguge">
                     <HBox.margin>
                        <Insets right="6.0" />
                     </HBox.margin>
                  </Label>
                  <ComboBox fx:id="comboBoxLanguage" prefHeight="25.0" prefWidth="202.0" />
               </children>
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_RIGHT">
               <children>
                  <Label fx:id="labelTimeout" text="%key.labelTimeout">
                     <HBox.margin>
                        <Insets right="6.0" />
                     </HBox.margin>
                  </Label>
                  <ComboBox fx:id="comboBoxTimeout" prefHeight="25.0" prefWidth="202.0" />
               </children>
            </HBox>
         </children>
         <BorderPane.margin>
            <Insets left="12.0" right="12.0" top="12.0" />
         </BorderPane.margin>
      </VBox>
   </center>
</BorderPane>
